class Solution:
    def josephus(self, n, k):
        if k == 2:
            i = n.bit_length()
            msb = 1 << i - 1
            return 2 * (n ^ msb) + 1
        def dfs(n, k):
            if n == 1:
                return 0
            return (dfs(n - 1, k) + k) % n
            
        return dfs(n, k) + 1
